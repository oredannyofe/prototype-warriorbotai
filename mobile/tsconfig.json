{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": ["react", "react-native", "node"],
    "baseUrl": ".",
    "paths": {"*": ["src/*"]},
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["src", "index.js"]
}
